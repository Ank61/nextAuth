generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:root@localhost:5432/trelloDemo"
}

model test {
  brand_id Int     @id @default(autoincrement())
  brand    String?
}

model Users {
  email  String @id @unique
  boards Boards[]
}

model Boards {
  id        String @id @default(cuid())
  title     String @unique
  workspace String
  emails    Users  @relation(fields: [email], references: [email])
  email     String 
}
